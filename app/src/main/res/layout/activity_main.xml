<?xml version="1.0" encoding="utf-8"?>

<layout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools">
    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:context=".propertyFilterByFacilities.presentation.propertyFilter.PropertyFacilityActivity">
        <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/filterRV"
                android:layout_width="0dp"
                android:layout_height="0dp"
                tools:itemCount="20"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="16dp"
                android:layout_marginEnd="16dp"
                tools:listitem="@layout/filter_list_item"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="16dp"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="16dp" android:rotation="0"/>

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>